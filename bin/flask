#!/opt/apps/resif/data/production/v0.1-20170602/default/software/lang/Python/2.7.13-intel-2017a/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Flask==1.0.3','console_scripts','flask'
__requires__ = 'Flask==1.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Flask==1.0.3', 'console_scripts', 'flask')()
    )
